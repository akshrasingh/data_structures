// Online C compiler to run C program online
#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *left;
    struct node *right;
};
struct node *head=NULL;
struct node *create(struct node *head ,int data)
{

    
        struct node *node_new=(struct node*)malloc(sizeof(struct node));
      
       
        node_new->data=data;
    struct node *p=head;
 if(head==NULL)
{
     node_new->left=NULL;
     node_new->right=NULL;
     head=node_new;
     p=head;
}
return head;
}
struct node *insert(struct node *head ,int data)
{
      struct node *node_new=(struct node*)malloc(sizeof(struct node));
      node_new->data=data;
      if(head==NULL)
      {
        return create(head,data);
      }
      else if(data>head->data)
      {
               head->right=insert(head->right,data);
      }
      else{
        head->left=insert(head->left,data);
      }
      return head;
}
struct node *inorder(struct node *head )
{
    if(head==NULL)
    {
        return 0 ;
    }  
      inorder(head->left);
    printf("%d->>",head->data);
    inorder(head->right);
    
  
}
struct node *preorder(struct node *head)
{
    if(head==NULL)
    {
        return 0;
    }
    printf("%d->>",head->data);
    preorder(head->left);
    preorder(head->right);
   
}
struct node *postorder(struct node *head)
{
    if(head==NULL)
    {
        return 0 ;
    }
    postorder(head->left);
    postorder(head->right);
    printf("%d->>",head->data);
 
}
int main()
{
    printf("enter the no of nodes you want -");
    int n;
    scanf("%d",&n);
   for(int i=0 ;i<n ;i++)
   {
    int data;
    printf("enter the data-");
        scanf("%d",&data);
        head=insert(head,data);
       
   }
   // Online C compiler to run C program online

    postorder(head);
     printf("\n");
     inorder(head);
   
    printf("\n");
    head=preorder(head);

}
